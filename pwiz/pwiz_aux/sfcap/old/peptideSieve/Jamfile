# Jamfile sample 


exe PeptideSieve
    : classify.cpp  
	config.cpp  
	digest.cpp  
	peptideSieve.cpp
	..//boost_filesystem 
	..//boost_program_options   
#    : <include>/stf/home/atrium/darren/local/include
    ;


# Note -- this could have been written on one line:
#   exe HelloTest : HelloTest.cpp Hello.cpp ;
# But remember that you need whitespace around the : and ;


# uncomment this install rule to automatically copy exe to temp directory

install exe : PeptideSieve : <location>bin ;


